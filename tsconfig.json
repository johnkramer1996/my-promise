{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "rootDir": "./src",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "lib": ["esnext"]
  }
}
